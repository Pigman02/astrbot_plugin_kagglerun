{
  "kaggle_username": {
    "type": "string",
    "description": "Kaggle 用户名"
  },
  "kaggle_api_key": {
    "type": "string",
    "description": "Kaggle API 密钥"
  },
  "kaggle_datasets": {
    "type": "list",
    "items": {
      "type": "string"
    },
    "description": "默认依赖的数据集列表（如 owner/dataset-slug）"
